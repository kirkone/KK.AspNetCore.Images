{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ImageProcessing": {
    "TargetFolder": "images/generated",
    "LosslessCompress": true,
    "Sizes": [
      {
        "Width": 450,
        "Quality": 70
      },
      {
        "Width": 600,
        "Quality": 80
      },
      {
        "Width": 750,
        "Quality": 95
      },
      {
        "Width": 1200,
        "Quality": 80
      },
      {
        "Width": 1500,
        "Quality": 95
      },
      {
        "Name": "custom",
        "Width": 1500,
        "Quality": 95
      }
    ]
  },
  "PictureTag": {
    "ImageFolder": "images/generated",
    "DoubbleSize": true,
    "Settings": [
      {
        "Name": "default",
        "Fallback": "750",
        "Sizes": [
          {
            "Width": 450,
            "Media": "(max-width: 450px)"
          },
          {
            "Width": 600,
            "Media": "(max-width: 600px)",
            "Zoom": [
              2
            ]
          },
          {
            "Width": 750,
            "Media": "(max-width: 750px)",
            "Zoom": [
              2
            ]
          },
          {
            "Width": 1200,
            "Media": "(max-width: 1200px)"
          },
          {
            "Width": 1500,
            "Media": "(min-width: 1201px)"
          },
          {
            "Name": "custom",
            "Width": 750,
            "Zoom": [
              2
            ]
          }
        ]
      }
    ]
  }
}